        <locationX>545</locationX>
        <locationY>539</locationY>
        <defaultConnectorLabel>NO</defaultConnectorLabel>
        <rules>
            <name>YES_ContentVersion</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>get_Content_Version</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Content_Document</targetReference>
            </connector>
            <label>YES</label>
        </rules>
    </decisions>
    <decisions>
        <name>is_extension_pdf</name>
        <label>is extension pdf ?</label>
        <locationX>781</locationX>
        <locationY>323</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>YES_EXTENSIONPDF</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record__Prior.Apttus__ContentVersionId__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Apttus__ContentVersionId__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Apttus__ContentVersionId__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>get_Content_Version</targetReference>
            </connector>
            <label>YES</label>
        </rules>
    </decisions>
    <decisions>
        <name>Upload_Signed_Document</name>
        <label>Upload Signed Document ?</label>
        <locationX>182</locationX>
        <locationY>1187</locationY>
        <defaultConnector>
            <targetReference>Update_content_version</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Uploaded_Outside</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Apttus__Action2__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Upload Signed Document</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_File_Name</targetReference>
            </connector>
            <label>Uploaded Outside CLM</label>
        </rules>
    </decisions>
    <description>BBH 22.08.24 W-2107-CLM - Standard naming for Uploaded Signed Documents</description>
    <environments>Default</environments>
    <formulas>
        <name>StageToApiname</name>
        <dataType>String</dataType>
        <expression>CASE({!$Record.Stage__c}, 
&apos;Conditions&apos;, &apos;6&apos;, 
&apos;Quote&apos;, &apos;1&apos;,
&apos;Lead&apos;, &apos;3&apos;,
&apos;Placement&apos;, &apos;2&apos;,
&apos;Signing&apos;, &apos;4&apos;
,&apos;5&apos;)</expression>
    </formulas>
    <formulas>
        <name>titleuploadsignedDoc</name>
        <dataType>String</dataType>
        <expression>&quot;Signed_&quot;+{!$Record.Tech_AgreementName__c}+&quot;_Final_&quot;+TEXT(TODAY())+&quot;_&quot;+{!$Record.Name}+&quot;_Signed outside CLM.pdf&quot;</expression>
    </formulas>
