    <decisions>
        <name>has_content_document</name>
        <label>has content document ?</label>
        <locationX>336</locationX>
        <locationY>755</locationY>
        <defaultConnectorLabel>NO</defaultConnectorLabel>
        <rules>
            <name>YES_ContentDoc</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Content_Document</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Content_Document.FileExtension</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>pdf</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>init_content_document_link_insert</targetReference>
            </connector>
            <label>YES</label>
        </rules>
    </decisions>
    <decisions>
        <name>has_content_version</name>
