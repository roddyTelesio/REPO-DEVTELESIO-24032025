    <formulas>
        <name>childAgreement_Name_Calc</name>
        <dataType>String</dataType>
        <expression>IF(NOT({!agreement.isAmended__c}),
&apos;B_&apos; &amp; TEXT({!agreement.Program__r.UwYear__c} ) + &apos;_&apos; &amp;{!agreement.Program__r.Name} &amp; &apos;_&apos; &amp; TRIM(SUBSTITUTE({!eachSigningRequest.RiskCarrier__r.Name}, &apos;(R)&apos;,&apos;&apos;)),
&apos;B_&apos; &amp; TEXT({!agreement.Program__r.UwYear__c} ) + &apos;_&apos; &amp;{!agreement.Program__r.Name} &amp; &apos;_&apos; &amp; TRIM(SUBSTITUTE({!eachSigningRequest.RiskCarrier__r.Name}, &apos;(R)&apos;,&apos;&apos;))+&apos; Amended &apos;+MID({!agreement.FullName__c},LEN({!agreement.FullName__c}),1 ))</expression>
    </formulas>
    <formulas>
        <name>childAgreement_Name_TRIM_Calc</name>
        <dataType>String</dataType>
        <expression>LEFT({!childAgreement_Name_Calc},80)</expression>
    </formulas>
